{
  "name": "test-synack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "fontsource-roboto": "^3.0.3",
    "keymirror-nested": "^1.0.3",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroll-hook": "^3.0.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "npm-run-all run:*",
    "run:app": "react-scripts start",
    "run:api": "json-server --watch db/index.js --port 3001",
    "lint": "eslint src/**/*.js",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "fix": "eslint --fix src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "faker": "^5.1.0",
    "jest-enzyme": "^7.1.2",
    "mutationobserver-shim": "^0.3.7",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/public/**",
      "!src/node_modules/**",
      "!src/coverage/**",
      "!src/serviceWorker.js",
      "!src/index.js",
      "!src/config/**/*.js",
      "!src/hooks/**/*.js",
      "!src/pages/**/*.js",
      "!src/commons/utils/validations.js",
      "!src/**/actionCreators.js",
      "!src/**/styles.js",
      "!src/**/types.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
